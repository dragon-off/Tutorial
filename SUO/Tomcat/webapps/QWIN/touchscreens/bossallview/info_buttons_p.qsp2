{"<td colspan="W" align=center>Окна</td></tr>"}
" <td class=boxTop colspan="W" align=center><h3 class=box>Окна<BR>(нажмите на окно которое хотите изменить)</h3></td></tr>"
I=1
"<TR>"
WHILE(I LE W)
BEGIN
"<TD class=boxMain width=35 align=center onclick=location.href='/touchscreens/bossallview/change_window.qsp?window="I"&priority="G"';> "
"<div id=link_1_1 class=listItem onmouseover=javascript:listOver(this,1); onmouseout=javascript:listOver(this,0);><B><FONT size=+1>"
I
"</FONT></B></DIV>"
"</TD>"
I=I+1
END
"</TR>"
M=30 {MAX GROUPS}
I=1
WHILE(I LE M)
BEGIN

	S=BUTTON_RWT(I)*2
	IF(S GE 3600)
		H=S/60/60
	T=(S-(H*60*60))/60
	C=S-(H*60*60)-(T*60)
	IF(((I/2)*2) EQ I)
	BEGIN
	"<TR bgcolor=#E2E2E2>"
	END
	ELSE
	"<TR>"
	"<TD align=center>"I"</TD><TD>"BUTTON_NAME(I)"</TD><TD align=center>"BUTTON_QUEUE(I)"</TD><TD align=center>"H LEN(2) ":" T LEN(2) ":" C LEN(2)"</TD>"
P=1
IF(G EQ 1)
H=0
IF(G EQ 2)
H=30
IF(G EQ 3)
H=60
IF(G EQ 4)
H=90
IF(G EQ 5)
H=120


WHILE(P LE W)
BEGIN
K=ARRAY(600+H+P)
{PROVERKA STROKI}
IF(STRSUB(K 31 31) NE "F")
	SETARRAY(600+H+P "000000000000000000000000000000F0") {31=F}
L=STRSUB(K I I)
IF(L EQ "1")
BEGIN
"<TD align=center bgcolor=#008000 title='" BUTTON_NAME(I)" (Окно №" P")'>"
END
ELSE IF(L EQ "2")
BEGIN
"<TD align=center bgcolor=#808000 title='" BUTTON_NAME(I)" (Окно №" P")'>"
END
ELSE IF(L EQ "3" )
BEGIN
"<TD align=center bgcolor=#ff0000 title='" BUTTON_NAME(I)" (Окно №" P")'>"
END
ELSE
BEGIN
"<TD align=center title='" BUTTON_NAME(I)" (Окно №" P")'>"
END
IF(L NE "0")
L
ELSE
"-"
"</TD>"
P=P+1
END
"</TR>"
	T=0
	H=0
	C=0
	I=I+1
END
""
"</table>"
"</TD><TD>"
