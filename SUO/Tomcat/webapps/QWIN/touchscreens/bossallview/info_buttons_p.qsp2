function alert1()
 {
  if(q.style.display=='none')
   {
    q.style.display='block';
    setTimeout('close()', 5000);
   }
  else 
   {
   q.style.display='none';
   }
 }

function close()
 {
  q.style.display='none';
qq.style.display='none';
  
}
</script>

