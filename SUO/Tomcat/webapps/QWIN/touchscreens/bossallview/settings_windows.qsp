    <body onLoad="startTimeout()">
<%
W=(${window}0)/10
S=(${status}0)/10
IF(W GT 0)
	SETARRAY(400+W S)	

G=(${spec}0)/10
IF(G GT 0)
	SETARRAY(7000+W G-1)	
%>
<SCRIPT LANGUAGE="javascript"> 
 
function startTimeout()
{
    setTimeout('reloadFunc()',30 * 1000 )
}
function reloadFunc()
{
    location.reload()
}
function openPage(p) 
{
    parent.contents.location.href=p;
}
</SCRIPT>
