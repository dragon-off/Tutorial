IF(L GT 0)
BEGIN
	IF((ARRAY(2950+W)+14) GT TIME_NO AND TIME_NO GE ARRAY(2950+W))
	BEGIN
		
		"LOGIN_ERROR^TIME"
	END
	ELSE
	BEGIN
		FUNC_LOGON(W L 0 0 0)
		SETARRAY(1130+W 0)
		SETARRAY(1180+W 0)
		SETARRAY(700+W 0)
		SETARRAY(2950+W TIME_NO)
		"loginname^"GS("LOGIN" "NAME"+COUNTER_LOGIN_ID(W) "UNKNOWN")"#"
		"js_run^run_qwin('next',999); wait_next();"
		"#"
		SETARRAY(950+W L)
	END
END
