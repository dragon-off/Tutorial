<head>
<%
W=(${workstation}0)/10
IF(W EQ 0)
	W=1
M=(${matter_true}0)/10
Q=(${matter}0)/10

IF(Q NE 0 AND M EQ 1)
BEGIN
	FUNC_MATTER(W Q 1 0 0)
	SETARRAY(9999 "TRUE")    {????????????????????????????}
END

P=(${priority}0)/10
IF(P GT 0)
	FUNC_PRIORITY(W P 0 0 0)
	
L=(${logincod}0)/10
IF(L GT 0)
	FUNC_LOGON(W L 0 0 0)
	
N=(${next}0)/10
IF(N GT 0)
	FUNC_NEXT(W COUNTER_PRIORITY_NUMBER(W) 0 0 0)
	
A=(${alert}0)/10
IF(A GT 0)
	FUNC_ALERT(W 1 0 0 0)
A=(${event}0)/10
	
I=(${insert_last}0)/10
IF(I GT 0)
	FUNC_INSERT_LAST(W 5 COUNTER_DISPLAY_NO(W) 30+I 0)
	
E=(${end}0)/10
IF(E GT 0)
	FUNC_END_CUSTOMER(W 0 0 0 0)
	
	V=(${vvod}0)/10
	IF(V EQ 1)
	BEGIN
		V=(${number}0)/10
		IF(V GT 0)
		BEGIN
			FUNC_LOGON(W V 0 0 0)
		END
	END

%>


<SCRIPT LANGUAGE="javascript"> 
function startTimeout()
{
setTimeout('reloadFunc()',2 * 1000 )

}
function reloadFunc()
{
 <%
   IF(Q GT 0 AND Q LT 50)
 	BEGIN
		 "location.href='?workstation="W"&matter="Q"&event="A"';"
	END
	ELSE
	BEGIN
		"location.href='?workstation="W"&event="A"';"
	END
	%>
}
function openPage(p) 
{
    parent.contents.location.href=p;
}
