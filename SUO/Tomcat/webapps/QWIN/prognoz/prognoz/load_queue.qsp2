WHILE(I LE 20)
BEGIN
	IF(ARRAY(6060+I)/ARRAY(530+I) GT 115*ARRAY(800+I)/100 OR ARRAY(6060+I)/ARRAY(530+I) LT 85*ARRAY(800+I)/100  )
	BEGIN
		I "^" ARRAY(800+I) "^" 0 "#"
	END
	ELSE
	BEGIN
		I "^" ARRAY(6060+I)/ARRAY(530+I) "^" 0 "#"
	END
	I=I+1
END
