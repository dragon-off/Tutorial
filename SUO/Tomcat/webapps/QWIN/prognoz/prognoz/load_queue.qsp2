{загрузка логики раб станций}
I=1
WHILE(I LE 30)
BEGIN
	IF(COUNTER_OPEN(I) EQ 1)
	BEGIN
		L=COUNTER_LOGIN_ID(I)
		B=1
		I "^"																	{0 counter}
		WHILE(B LE 20)
		BEGIN
			 STRSTN(STRSUB(ARRAY(600+I) B B))*STRSTN(STRSUB(ARRAY(1100+B) L L))	{1 MATRIX COUNTER*LOGIN}
			B=B+1
		END
		"^" STRSUB(ARRAY(1121) L L)  {2 VIP}
		"^" I "^" {3 counter}
		
	"#"
	END
I=I+1
END

