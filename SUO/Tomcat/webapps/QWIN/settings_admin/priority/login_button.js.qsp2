<%
R=(${reset}0)/10
IF(R EQ 0)
	R=0
IF(R EQ 1)
BEGIN
	I=1
	WHILE(I LE 30)
	BEGIN				
		SETARRAY(1100+I "")
		I=I+1
	END
END
I=1
L="" {SHOW LOGIN}
B="" {SHOW BUTTON}
WHILE(I LE 13)
BEGIN
	IF(ARRAY(1000+I) EQ 0 AND I LE 10)
	BEGIN
		IF(B EQ "")
			B=""+I
		ELSE
		BEGIN
			B=B+","+I
		END
	END

	IF(I LE 13) 
	BEGIN
		
		"var operation_"I"='"ARRAY(1100+I)"';\n"
	END
	ELSE
	BEGIN
		"var operation_"I"=null;\n"
	END
	I=I+1
END
I=1
S=ARRAY(1100)
WHILE(I LE MAX_LOGINS)
BEGIN
	IF(STRSUB(ARRAY(1100) I I) EQ "0")
		IF(L EQ "")
			L=""+I
		ELSE
		BEGIN
			L=L+","+I
		END
	I=I+1
END
"var max_logins=" MAX_LOGINS ";\n"
"var show_login = ["L"];\n"
"var show_button = ["B",11,12,13];"
%>	 