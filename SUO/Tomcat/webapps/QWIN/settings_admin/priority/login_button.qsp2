<TR>
<%
U=11
I=1
WHILE(I LT U)
BEGIN
	"<TD width=70 class=boxTopBlue align=center ><div ><B><FONT size=-1>"
	ARRAY(2000+I)  
	"</FONT></B></DIV></TD>"
	I=I+1
END
{"<td width=70 align=center >Спец. регистрация</td><td width=70 align=center >Вызов из очереди</td>"}
"</TR>"
M=MAX_LOGINS
IF(M GT 50)
	M=50
I=1
WHILE(I LE M)
BEGIN
	IF(STRSUB(ARRAY(1100) I I) EQ "0")
	BEGIN
		IF(((I/2)*2) EQ I)
			"<TR bgcolor=#E2E2E2>"
		ELSE
			"<TR>"
		"<TD align=center style='display:none;'>"I"</TD><TD> "LOGIN_NAME(I)"</TD>"
		P=1
		WHILE(P LE U)
		BEGIN
			"<TD id=win_"P"-"I" align=center></TD>"
			P=P+1
		END
		"<td id=win_12-"I" align=center></td><td id=win_13-"I" align=center></td></TR>"
		T=0
		C=0
	END 
	I=I+1
END
%>
