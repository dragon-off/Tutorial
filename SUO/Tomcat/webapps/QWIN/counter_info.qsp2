<%
I=1
WHILE(I LE MAX_COUNTERS)
BEGIN
	I ";"
	IF(COUNTER_CUSTOMER_PRESENT(I) EQ 1)
	BEGIN
		BUTTON_LETTER(COUNTER_CUSTOMER_ITEM(I ITEM_ENTER_BUTTON)) COUNTER_CUSTOMER_ITEM(I ITEM_NUMBER) LEN(3)
		";"
		COUNTER_TRANSACTION_TIME_NOW(I)*2
		";"
		PRINT_CLOCK_SEC(COUNTER_TRANSACTION_TIME_NOW(I))
	END ELSE
		"NO CLIENT;0;00:00:00"
	"\r\n"
	I=I+1
END
%>